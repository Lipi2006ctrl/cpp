#include <iostream>
using namespace std;

template <class T>
void swapValues(T &a, T &b) {
    T temp = a;
    a = b;
    b = temp;
}

int main() {
    int x=5, y=10;
    float p=3.5, q=7.2;
    string s1="Hello", s2="World";

    swapValues(x, y);
    swapValues(p, q);
    swapValues(s1, s2);

    cout << "Swapped integers: " << x << ", " << y << endl;
    cout << "Swapped floats: " << p << ", " << q << endl;
    cout << "Swapped strings: " << s1 << ", " << s2 << endl;
    return 0;
}
