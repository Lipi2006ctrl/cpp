#include <iostream>
using namespace std;

int main() {
    int arr[5] = {10, 20, 30, 40, 50};
    int index;
    cout << "Enter index to access (0-4): ";
    cin >> index;

    try {
        if (index < 0 || index >= 5)
            throw out_of_range("Error: Index out of bounds!");
        cout << "Element at index " << index << ": " << arr[index] << endl;
    } catch (out_of_range &e) {
        cout << e.what() << endl;
    }

    return 0;
}
