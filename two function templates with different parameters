#include <iostream>
using namespace std;

template <typename T>
void print(T a) {
    cout << "One parameter: " << a << endl;
}

template <typename T1, typename T2>
void print(T1 a, T2 b) {
    cout << "Two parameters: " << a << " and " << b << endl;
}

int main() {
    print(5);          // Calls first template
    print(10, 20);     // Calls second template
    print('A', 3.14);  // Calls second template
    return 0;
}
