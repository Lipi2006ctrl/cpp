#include <iostream>
using namespace std;

int main() {
    int a = 10;
    double b = 3.5;

    
    double result1 = a + b;   

   
    int result2 = (int)b + a;  

    
    double result3 = static_cast<double>(a) / 4;

    cout << "Implicit Casting (a + b): " << result1 << endl;
    cout << "Explicit Casting ((int)b + a): " << result2 << endl;
    cout << "Using static_cast<double>(a) / 4: " << result3 << endl;

    return 0;
}
