#include <iostream>
#include <cmath>
using namespace std;

double calculateSquareRoot(double n) {
    if (n < 0)
        throw invalid_argument("Error: Cannot calculate square root of negative number!");
    return sqrt(n);
}

int main() {
    double num;
    cout << "Enter a number: ";
    cin >> num;

    try {
        double result = calculateSquareRoot(num);
        cout << "Square root: " << result << endl;
    } catch (invalid_argument &e) {
        cout << e.what() << endl;
    }

    return 0;
}
