#include <iostream>
using namespace std;

template <typename T1, typename T2>
void display(T1 a, T2 b) {
    cout << "a = " << a << ", b = " << b << endl;
}

int main() {
    display(10, 3.14);      // int and double
    display('A', "Hello");  // char and string
    display(5.5, 2);        // double and int
    return 0;
}
