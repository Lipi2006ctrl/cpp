#include <iostream>
using namespace std;

int main() {
    int arr[] = {10, 20, 30, 40, 50};
    int n = sizeof(arr)/sizeof(arr[0]);
    int key;
    bool found = false;

    cout << "Enter element to search: ";
    cin >> key;

    for (int i = 0; i < n; i++) {
        if (arr[i] == key) {
            cout << key << " found at position " << i+1 << endl;
            found = true;
            break;
        }
    }

    if (!found)
        cout << key << " not found in the array." << endl;

    return 0;
}
